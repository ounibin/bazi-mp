<van-action-sheet show="{{ showCalendar }}" title="标题" bind:close="onCloseCalendarActionSheet">
  <van-datetime-picker type="datetime" value="{{ currentDate }}" min-date="{{ minDate }}" max-date="{{ maxDate }}"
    bind:confirm="onInputDate" />
</van-action-sheet>